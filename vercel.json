{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": ":url(^(?!.*\\.).*[^/]$)",
      "destination": ":url.html",
      "statusCode": 301
    },
    {
      "source": "/docs/resources/guidelines/core:url(^(?!.*\\.).*[^/]$)",
      "destination": "/docs/resources/guidelines/code/core:url.html",
      "statusCode": 301
    }
  ]
}